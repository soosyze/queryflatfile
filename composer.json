{
    "name": "soosyze/queryflatfile",
    "description": "The Queryflatfile is PHP library for simple database not SQL",
    "type": "library",
    "keywords": ["php", "library", "php-library", "nosql", "db", "database", "flat file", "db flat file", "query-builder", "composer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu NOÃ‹L",
            "email": "mathieu@soosyze.com",
            "homepage": "http://mathieu-noel.fr"
        }
    ],
    "support": {
        "email": "mathieu@soosyze.com"
    },
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2",
        "phpstan/phpstan": "^0.12.85",
        "phpstan/phpstan-phpunit": "^0.12.18",
        "phpunit/phpunit": "^8.5"
    },
    "suggest": {
        "phpdocumentor/phpdocumentor": "To generate documentation.",
        "phpmetrics/phpmetrics": "To generate a code status report in HTML format."
    },
    "autoload": {
        "psr-4": {
            "Queryflatfile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Queryflatfile\\Tests\\": "tests"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
